import sys

lines = open(sys.argv[1], 'r').readlines()
search1 = 'epoch, neuron_units,'
search2 = '7, 252,'
copy = 0
for i, line in enumerate(lines):
    if search1 in line:
        copy = 1
    if search2 in line:
        break
    if copy == 1:
        with open(sys.argv[2], 'a') as f:
            f.write(line)

if i < len(lines) - 1:
    with open(sys.argv[3], 'w') as f:
        f.write(''.join(lines[i:]))
        
